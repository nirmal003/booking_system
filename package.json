{
  "name": "ticket-booking-system-api",
  "version": "1.0.0",
  "description": "RESTful API for Event Management and Ticket Booking System with MySQL (Sequelize) and MongoDB (Mongoose)",
  "main": "bin/www",
  "scripts": {
    "start": "node bin/www",
    "dev": "nodemon bin/www",
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate": "node bin/migrate",
    "migrate:undo": "node bin/rollback",
    "migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "migrate:status": "npx sequelize-cli db:migrate:status",
    "seed": "node bin/seed",
    "seed:undo": "npx sequelize-cli db:seed:undo:all",
    "reset": "node bin/reset",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,json,md}\""
  },
  "keywords": [
    "event",
    "ticket",
    "booking",
    "api",
    "rest",
    "sequelize",
    "mongoose",
    "mysql",
    "mongodb",
    "jwt",
    "authentication",
    "migrations"
  ],
  "author": "Nirmal",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.6.5",
    "sequelize": "^6.35.2",
    "xss-clean": "^0.1.4",
    "mongoose": "^8.0.3",
    "joi": "^17.11.0",
    "hpp": "^0.2.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.2",
    "eslint": "^8.55.0",
    "prettier": "^3.1.1"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/ticket-booking-system"
  }
}